<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>YouTube SEO System - Configuration Files</title>
    <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@6.4.0/css/all.min.css">
    <style>
        .code-block {
            background: #1a1a1a;
            color: #f8f8f2;
            font-family: 'Fira Code', 'Monaco', 'Menlo', monospace;
            border-radius: 8px;
            overflow-x: auto;
        }
        .copy-btn {
            transition: all 0.2s ease;
        }
        .copy-btn:hover {
            transform: translateY(-1px);
        }
    </style>
</head>
<body class="bg-gray-50 min-h-screen">
    <div class="container mx-auto px-4 py-8">
        <div class="text-center mb-8">
            <h1 class="text-3xl font-bold text-gray-800 mb-2">
                <i class="fas fa-cogs text-blue-600 mr-3"></i>
                YouTube SEO System - Configuration Files
            </h1>
            <p class="text-gray-600">Package dependencies and environment configuration for your production server</p>
        </div>

        <!-- Package.json File -->
        <div class="bg-white rounded-lg shadow-lg p-6 mb-8">
            <div class="flex items-center justify-between mb-4">
                <h2 class="text-xl font-bold text-gray-800">
                    <i class="fas fa-file-code text-green-600 mr-2"></i>
                    package.json
                </h2>
                <button onclick="copyToClipboard('package-json')" class="copy-btn bg-blue-600 text-white px-4 py-2 rounded-lg hover:bg-blue-700">
                    <i class="fas fa-copy mr-2"></i>Copy
                </button>
            </div>
            <div class="code-block p-4" id="package-json">
<pre>{
  "name": "youtube-seo-backend",
  "version": "1.0.0",
  "description": "Real YouTube SEO optimization backend with AI-powered transcript extraction and content optimization",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "youtube",
    "seo",
    "transcript",
    "ai",
    "openai",
    "content-optimization"
  ],
  "author": "AFFILIAXA",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "youtube-transcript": "^1.2.1",
    "openai": "^4.20.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2",
    "ytdl-core": "^4.11.5",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}</pre>
            </div>
        </div>

        <!-- .env.example File -->
        <div class="bg-white rounded-lg shadow-lg p-6 mb-8">
            <div class="flex items-center justify-between mb-4">
                <h2 class="text-xl font-bold text-gray-800">
                    <i class="fas fa-key text-yellow-600 mr-2"></i>
                    .env.example
                </h2>
                <button onclick="copyToClipboard('env-example')" class="copy-btn bg-blue-600 text-white px-4 py-2 rounded-lg hover:bg-blue-700">
                    <i class="fas fa-copy mr-2"></i>Copy
                </button>
            </div>
            <div class="code-block p-4" id="env-example">
<pre># Server Configuration
PORT=3000
NODE_ENV=production

# API Keys (REQUIRED)
OPENAI_API_KEY=sk-your-openai-api-key-here
YOUTUBE_API_KEY=your-youtube-data-api-key-here

# CORS Configuration
ALLOWED_ORIGINS=https://www.affiliaxa.com,https://affiliaxa.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# OpenAI Configuration
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=2000
OPENAI_TEMPERATURE=0.7

# YouTube Configuration
YOUTUBE_REGION=US
YOUTUBE_LANGUAGE=en

# Security
API_SECRET=your-random-secret-key-here
ENCRYPTION_KEY=your-32-character-encryption-key

# Monitoring (Optional)
LOG_LEVEL=info
ENABLE_ANALYTICS=true

# Third-party Services (Optional)
RAPIDAPI_KEY=your-rapidapi-key-for-backup-services</pre>
            </div>
        </div>

        <!-- Installation Instructions -->
        <div class="bg-white rounded-lg shadow-lg p-6 mb-8">
            <h2 class="text-xl font-bold text-gray-800 mb-4">
                <i class="fas fa-rocket text-purple-600 mr-2"></i>
                Installation Instructions
            </h2>
            <div class="space-y-4">
                <div class="bg-blue-50 border-l-4 border-blue-500 p-4">
                    <h3 class="font-bold text-blue-800 mb-2">Step 1: Initialize Project</h3>
                    <div class="code-block p-3">
                        <code>npm init -y</code>
                    </div>
                </div>
                
                <div class="bg-green-50 border-l-4 border-green-500 p-4">
                    <h3 class="font-bold text-green-800 mb-2">Step 2: Install Dependencies</h3>
                    <div class="code-block p-3">
                        <code>npm install express cors youtube-transcript openai helmet express-rate-limit dotenv node-fetch ytdl-core axios cheerio express-validator compression morgan</code>
                    </div>
                </div>
                
                <div class="bg-yellow-50 border-l-4 border-yellow-500 p-4">
                    <h3 class="font-bold text-yellow-800 mb-2">Step 3: Install Dev Dependencies</h3>
                    <div class="code-block p-3">
                        <code>npm install --save-dev nodemon</code>
                    </div>
                </div>
                
                <div class="bg-purple-50 border-l-4 border-purple-500 p-4">
                    <h3 class="font-bold text-purple-800 mb-2">Step 4: Create Environment File</h3>
                    <div class="code-block p-3">
                        <code>cp .env.example .env</code>
                    </div>
                    <p class="text-sm text-purple-700 mt-2">Then edit .env with your actual API keys</p>
                </div>
            </div>
        </div>

        <!-- API Keys Guide -->
        <div class="bg-white rounded-lg shadow-lg p-6">
            <h2 class="text-xl font-bold text-gray-800 mb-4">
                <i class="fas fa-key text-red-600 mr-2"></i>
                Required API Keys
            </h2>
            <div class="grid md:grid-cols-2 gap-6">
                <div class="border rounded-lg p-4">
                    <h3 class="font-bold text-gray-800 mb-2">OpenAI API Key</h3>
                    <p class="text-gray-600 text-sm mb-2">Required for AI-powered SEO optimization</p>
                    <a href="https://platform.openai.com/api-keys" target="_blank" class="text-blue-600 hover:underline text-sm">
                        <i class="fas fa-external-link-alt mr-1"></i>Get OpenAI API Key
                    </a>
                </div>
                <div class="border rounded-lg p-4">
                    <h3 class="font-bold text-gray-800 mb-2">YouTube Data API Key</h3>
                    <p class="text-gray-600 text-sm mb-2">Required for video metadata extraction</p>
                    <a href="https://console.cloud.google.com/apis/credentials" target="_blank" class="text-blue-600 hover:underline text-sm">
                        <i class="fas fa-external-link-alt mr-1"></i>Get YouTube API Key
                    </a>
                </div>
            </div>
        </div>
    </div>

    <script>
        function copyToClipboard(elementId) {
            const element = document.getElementById(elementId);
            const text = element.textContent || element.innerText;
            
            navigator.clipboard.writeText(text).then(function() {
                // Show success message
                const btn = event.target.closest('button');
                const originalText = btn.innerHTML;
                btn.innerHTML = '<i class="fas fa-check mr-2"></i>Copied!';
                btn.classList.add('bg-green-600');
                btn.classList.remove('bg-blue-600');
                
                setTimeout(() => {
                    btn.innerHTML = originalText;
                    btn.classList.remove('bg-green-600');
                    btn.classList.add('bg-blue-600');
                }, 2000);
            }).catch(function(err) {
                console.error('Could not copy text: ', err);
                alert('Failed to copy. Please select and copy manually.');
            });
        }
    </script>
</body>
</html>
